extends ../layout

block content
  h1 Hosting List
  br
  div(class='panel panel-default')
    table(class='table table-hover')
      th Name
      th Brief Description
      th Date
      th Location
      each project in data
        tr
          td
            a(href='/project/#{project.objectId}') #{project.name}
          td= project.brief
          td= project.date_string
          td= project.location
